{
	"extends": "airbnb-base",
	"parser": "@babel/eslint-parser",
	"env": {
		"es6": true,
		"browser": true,
		"jest": true
	},
	"parserOptions": {
		"ecmaVersion": 2018,
		"sourceType": "module"
	},
	"rules": {
		"linebreak-style": 0,
		"no-tabs": 0,
		"indent": 0,
		"spaced-comment": 0,
		"no-console": 0
	}
}